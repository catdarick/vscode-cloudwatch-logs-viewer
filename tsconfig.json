{
  "compilerOptions": {
    "target": "ES2020",
  "module": "Node16",
    "lib": ["ES2020"],
    "moduleResolution": "node16",
    "outDir": "out",
    "rootDir": "src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "sourceMap": true,
    "types": ["node", "vscode"]
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist", "out"]
}
